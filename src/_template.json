{
	"name": "{{name}}",
	"type": "{{type}}",
	"colors": {
		"editorPane.background": "{{negative}}",
		"editor.background": "{{b-25}}",
		"editor.foreground": "{{n}}",
		"editor.lineHighlightBackground": "{{b-33}}",
		"editor.selectionBackground": "{{b}}",
		"editorRuler.foreground": "{{b}}",
		"editorLineNumber.foreground": "{{b}}",
		"editorLineNumber.activeForeground": "{{n-50}}",
		"editorBracketMatch.background": "{{n-50}}",
		"editorBracketMatch.border": "#00000000",
		"editorIndentGuide.background": "{{b-50}}",
		"editorIndentGuide.activeBackground": "{{b}}",
		
		"statusBar.noFolderBackground": "{{negative}}",
		"statusBar.noFolderForeground": "{{n-50}}",
		"statusBar.background": "{{b-50}}",
		"statusBar.foreground": "{{n-50}}",
		"statusBar.debuggingBackground": "{{v2-25}}",
		"statusBar.debuggingForeground": "{{v2-75}}",
		"statusBarItem.remoteBackground": "{{l3-25}}",
		"statusBarItem.remoteForeground": "{{l3}}",
		
		"activityBar.background": "{{negative}}",
		"activityBar.foreground": "{{n-67}}",
		"activityBar.inactiveForeground": "{{n-50}}",
		"activityBar.activeBackground": "{{b-33}}",
		"activityBarBadge.background": "{{v3-75}}",
		"activityBarBadge.foreground": "{{n}}",
		"sideBar.background": "{{b-33}}",
		"sideBar.foreground": "{{n-67}}",
		"sideBar.border": "{{negative}}",
		"sideBarTitle.foreground": "{{l3}}",
		"sideBarSectionHeader.foreground": "{{l1-75}}",
		"sideBarSectionHeader.background": "{{b-50}}",
		"list.focusBackground": "{{v3-50}}",
		"list.focusForeground": "{{n}}",
		"list.activeSelectionBackground": "{{v3-50}}",
		"list.activeSelectionForeground": "{{n}}",
		"list.inactiveSelectionBackground": "{{b-67}}",
		"list.highlightForeground": "{{v3}}",

		"dropdown.background": "{{l3-33}}",
		"dropdown.border": "{{l3-50}}",
		"dropdown.listBackground": "{{b-50}}",

		"input.background": "{{v3-33}}",
		"input.border": "{{v3-50}}",
		"input.placeholderForeground": "{{v3-67}}",
		"focusBorder": "{{n-33}}",
		"inputOption.activeBorder": "{{v3-67}}",
		"inputOption.activeBackground": "{{v3-67}}",

		"breadcrumb.background": "{{b-50}}",
		"breadcrumb.foreground": "{{n-67}}",
		"breadcrumb.activeSelectionForeground": "{{n}}",
		"breadcrumb.focusForeground": "{{n}}",

		"titleBar.activeBackground": "{{b-50}}",
		"titleBar.activeForeground": "{{n-67}}",
		"menu.background": "{{b-50}}",
		"menu.foreground": "{{n-75}}",
		"menu.separatorBackground": "{{n-50}}",
		"menu.selectionBackground": "{{v3-75}}",
		"menu.selectionForeground": "{{n}}",
		"menubar.selectionBackground": "{{v3-50}}",
		"menubar.selectionForeground": "{{n}}",
		"quickInput.background": "{{b-50}}",
		"quickInput.foreground": "{{n-75}}",
		"editorHoverWidget.background": "{{b-50}}",
		"editorHoverWidget.foreground": "{{n-75}}",
		"editorHoverWidget.border": "{{n-33}}",

		"panel.background": "{{b-33}}",
		"panel.border": "{{negative}}",
		"panelTitle.activeForeground": "{{n-67}}",
		"panelTitle.activeBorder": "{{l3}}",

		"editorGroupHeader.tabsBackground": "{{b-25}}",
		"editorGroupHeader.tabsBorder": "{{b-50}}",
		"tab.activeBackground": "{{b-50}}",
		"tab.activeForeground": "{{n-67}}",
		"tab.activeBorderTop": "{{b}}",
		"tab.inactiveBackground": "{{b-25}}",
		"tab.inactiveForeground": "{{n-33}}",
		"tab.unfocusedActiveBackground": "{{b-50}}",
		"tab.unfocusedActiveForeground": "{{n-33}}",
		"tab.unfocusedActiveBorderTop": "{{b-50}}",
		"tab.unfocusedInactiveBackground": "{{b-25}}",
		"tab.unfocusedInactiveForeground": "{{n-25}}",
		"tab.border": "{{b-50}}",

		"badge.background": "{{v3-50}}",
		"badge.foreground": "{{n}}",
		"button.background": "{{l3-75}}",
		"button.foreground": "{{n}}",

		"textPreformat.foreground": "{{v3}}",
		"textLink.foreground": "{{l3}}"
	},
	"tokenColors": [
		{
			"name": "Comments\n",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "{{n-50}}"
			}
		},
		{
			"name": "Strings",
			"scope": [
				"string",
				"punctuation.definition.string.begin",
				"punctuation.definition.string.end",
				"markup.inline.raw.string"
			],
			"settings": {
				"foreground": "{{v3}}"
			}
		},
		{
			"name": "Strings: Escape Sequences",
			"scope": [
				"constant.character.escape"
			],
			"settings": {
				"foreground": "{{v4}}"
			}
		},
		{
			"name": "Very Logical",
			"scope": [
				"keyword.control",
				"keyword.operator.new",
				"keyword.other",
				"keyword.operator.expression",
				"string.regexp support.other.match.begin",
				"string.regexp support.other.match.end"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "{{l3}}"
			}
		},
		{
			"name": "Very Logical (Undecorated)",
			"scope": [
				"punctuation",
				"keyword.operator.comparison",
				"keyword.operator.relational",
				"keyword.operator.logical",
				"keyword.operator.or",
				"keyword.operator.disjunction",
				"string.regexp keyword.operator.lookahead",
				"meta.brace"
			],
			"settings": {
				"foreground": "{{l3}}"
			}
		},
		{
			"name": "Slightly Logical",
			"scope": [
				"constant.language",
				"source.css support.constant"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "{{l1}}"
			}
		},
		{
			"name": "Slightly Logical (Undecorated)",
			"scope": [
				"entity.name.function",
				"support.function",
				"entity.name.tag",
				"markup.heading",
				"string.regexp keyword.operator.quantifier"
			],
			"settings": {
				"foreground": "{{l1}}"
			}
		},
		{
			"name": "Neutral",
			"scope": [
				"meta.template.expression",
				"string.unquoted.heredoc.php",
				"punctuation.definition.variable.php",
				"variable.other.php",
				"meta.embedded.block.php",
				"meta.interpolation",
				"text.html.php",
				"variable.other.php"
			],
			"settings": {
				"foreground": "{{n}}"
			}
		},
		{
			"name": "Very Valuey (Undecorated)",
			"scope": [
				"storage",
				"keyword.other.namespace",
				"keyword.operator.assignment",
				"keyword.operator.increment",
				"keyword.operator.decrement",
				"punctuation.definition.bold",
				"punctuation.definition.italic",
				"punctuation.definition.underline",
				"punctuation.definition.strikethrough",
				"meta.brace.square",
				"punctuation.definition.begin.bracket.square",
				"punctuation.definition.end.bracket.square"
			],
			"settings": {
				"foreground": "{{v3}}"
			}
		},
		{
			"name": "Moderately Valuey",
			"scope": [
				"entity.name.type",
				"entity.other.inherited-class",
				"support.type",
				"support.class",
				"keyword.type"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "{{v2}}"
			}
		},
		{
			"name": "Moderately Valuey (Undecorated)",
			"scope": [
				"keyword.operator.type",
				"keyword.operator.optional",
				"string.regexp constant.other.character-class",
				"string.regexp punctuation.definition.character-class",
				"string.regexp punctuation.character.set",
				"string.regexp support.other.escape.special",
				"string.regexp support.other.match.any"
			],
			"settings": {
				"foreground": "{{v2}}"
			}
		},
		{
			"name": "Slightly Valuey (Undecorated)",
			"scope": [
				"constant.numeric",
				"keyword.other.unit",
				"variable.other.property",
				"variable.other.constant.property",
				"support.variable.property",
				"meta.object-literal.key",
				"entity.other.attribute-name",
				"string.regexp constant.other.character-class.set",
				"string.regexp constant.other.character-class.range",
				"string.regexp constant.character.set",
				"markup.bold",
				"markup.italic",
				"markup.underline",
				"markup.strikethrough"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "{{v1}}"
			}
		},
		{
			"name": "Markup: italic",
			"scope": [
				"markup.italic"
			],
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"name": "Markup: bold",
			"scope": [
				"markup.bold"
			],
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markup: underline",
			"scope": [
				"markup.underline"
			],
			"settings": {
				"fontStyle": "underline"
			}
		},
		{
			"name": "Markup: strikethrough",
			"scope": [
				"markup.strikethrough"
			],
			"settings": {
				"fontStyle": "strikethrough"
			}
		},
		{
			"name": "Markup: heading",
			"scope": [
				"markup.heading"
			],
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"scope": "token.info-token",
			"settings": {
				"foreground": "#6796e6"
			}
		},
		{
			"scope": "token.warn-token",
			"settings": {
				"foreground": "#cd9731"
			}
		},
		{
			"scope": "token.error-token",
			"settings": {
				"foreground": "#f44747"
			}
		},
		{
			"scope": "token.debug-token",
			"settings": {
				"foreground": "#b267e6"
			}
		}
	],
	"semanticHighlighting": true,
	"semanticTokenColors": {
		"parameter": {
			"italic": true
		},
		"macro": {
			"foreground": "{{v1}}",
			"italic": true
		},
		"enumMember": {
			"foreground": "{{l1}}",
			"italic": true
		},
		"variable.global": {
			"foreground": "{{v1}}"
		}
	}
}