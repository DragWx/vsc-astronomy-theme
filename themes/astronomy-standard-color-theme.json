{
	"name": "Astronomy",
	"type": "dark",
	"colors": {
		"editorPane.background": "#000000",
		"editor.background": "#111111",
		"editor.foreground": "#EEEEEE",
		"editor.lineHighlightBackground": "#161616",
		"editor.selectionBackground": "#444444",
		"editorRuler.foreground": "#444444",
		"editorLineNumber.foreground": "#444444",
		"editorLineNumber.activeForeground": "#777777",
		"editorBracketMatch.background": "#777777",
		"editorBracketMatch.border": "#00000000",
		"editorIndentGuide.background": "#222222",
		"editorIndentGuide.activeBackground": "#444444",
		
		"statusBar.noFolderBackground": "#000000",
		"statusBar.noFolderForeground": "#777777",
		"statusBar.background": "#222222",
		"statusBar.foreground": "#777777",
		"statusBar.debuggingBackground": "#3b261d",
		"statusBar.debuggingForeground": "#b27259",
		"statusBarItem.remoteBackground": "#0c223b",
		"statusBarItem.remoteForeground": "#3388EE",
		
		"activityBar.background": "#000000",
		"activityBar.foreground": "#9f9f9f",
		"activityBar.inactiveForeground": "#777777",
		"activityBar.activeBackground": "#161616",
		"activityBarBadge.background": "#a54c59",
		"activityBarBadge.foreground": "#EEEEEE",
		"sideBar.background": "#161616",
		"sideBar.foreground": "#9f9f9f",
		"sideBar.border": "#000000",
		"sideBarTitle.foreground": "#3388EE",
		"sideBarSectionHeader.foreground": "#7fa5bf",
		"sideBarSectionHeader.background": "#222222",
		"list.focusBackground": "#6e333b",
		"list.focusForeground": "#EEEEEE",
		"list.activeSelectionBackground": "#6e333b",
		"list.activeSelectionForeground": "#EEEEEE",
		"list.inactiveSelectionBackground": "#2d2d2d",
		"list.highlightForeground": "#DD6677",

		"dropdown.background": "#102c4e",
		"dropdown.border": "#194477",
		"dropdown.listBackground": "#222222",

		"input.background": "#482127",
		"input.border": "#6e333b",
		"input.placeholderForeground": "#94444f",
		"focusBorder": "#4e4e4e",
		"inputOption.activeBorder": "#94444f",
		"inputOption.activeBackground": "#94444f",

		"breadcrumb.background": "#222222",
		"breadcrumb.foreground": "#9f9f9f",
		"breadcrumb.activeSelectionForeground": "#EEEEEE",
		"breadcrumb.focusForeground": "#EEEEEE",

		"titleBar.activeBackground": "#222222",
		"titleBar.activeForeground": "#9f9f9f",
		"menu.background": "#222222",
		"menu.foreground": "#b2b2b2",
		"menu.separatorBackground": "#777777",
		"menu.selectionBackground": "#a54c59",
		"menu.selectionForeground": "#EEEEEE",
		"menubar.selectionBackground": "#6e333b",
		"menubar.selectionForeground": "#EEEEEE",
		"quickInput.background": "#222222",
		"quickInput.foreground": "#b2b2b2",
		"editorHoverWidget.background": "#222222",
		"editorHoverWidget.foreground": "#b2b2b2",
		"editorHoverWidget.border": "#4e4e4e",

		"panel.background": "#161616",
		"panel.border": "#000000",
		"panelTitle.activeForeground": "#9f9f9f",
		"panelTitle.activeBorder": "#3388EE",

		"editorGroupHeader.tabsBackground": "#111111",
		"editorGroupHeader.tabsBorder": "#222222",
		"tab.activeBackground": "#222222",
		"tab.activeForeground": "#9f9f9f",
		"tab.activeBorderTop": "#444444",
		"tab.inactiveBackground": "#111111",
		"tab.inactiveForeground": "#4e4e4e",
		"tab.unfocusedActiveBackground": "#222222",
		"tab.unfocusedActiveForeground": "#4e4e4e",
		"tab.unfocusedActiveBorderTop": "#222222",
		"tab.unfocusedInactiveBackground": "#111111",
		"tab.unfocusedInactiveForeground": "#3b3b3b",
		"tab.border": "#222222",

		"badge.background": "#6e333b",
		"badge.foreground": "#EEEEEE",
		"button.background": "#2666b2",
		"button.foreground": "#EEEEEE",

		"textPreformat.foreground": "#DD6677",
		"textLink.foreground": "#3388EE"
	},
	"tokenColors": [
		{
			"name": "Comments\n",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#777777"
			}
		},
		{
			"name": "Strings",
			"scope": [
				"string",
				"punctuation.definition.string.begin",
				"punctuation.definition.string.end",
				"markup.inline.raw.string"
			],
			"settings": {
				"foreground": "#DD6677"
			}
		},
		{
			"name": "Strings: Escape Sequences",
			"scope": [
				"constant.character.escape"
			],
			"settings": {
				"foreground": "#AA4499"
			}
		},
		{
			"name": "Very Logical",
			"scope": [
				"keyword.control",
				"keyword.operator.new",
				"keyword.other",
				"keyword.operator.expression",
				"string.regexp support.other.match.begin",
				"string.regexp support.other.match.end"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#3388EE"
			}
		},
		{
			"name": "Very Logical (Undecorated)",
			"scope": [
				"punctuation",
				"keyword.operator.comparison",
				"keyword.operator.relational",
				"keyword.operator.logical",
				"keyword.operator.or",
				"keyword.operator.disjunction",
				"string.regexp keyword.operator.lookahead",
				"meta.brace"
			],
			"settings": {
				"foreground": "#3388EE"
			}
		},
		{
			"name": "Slightly Logical",
			"scope": [
				"constant.language",
				"source.css support.constant"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#AADDFF"
			}
		},
		{
			"name": "Slightly Logical (Undecorated)",
			"scope": [
				"entity.name.function",
				"support.function",
				"entity.name.tag",
				"markup.heading",
				"string.regexp keyword.operator.quantifier"
			],
			"settings": {
				"foreground": "#AADDFF"
			}
		},
		{
			"name": "Neutral",
			"scope": [
				"meta.template.expression",
				"string.unquoted.heredoc.php",
				"punctuation.definition.variable.php",
				"variable.other.php",
				"meta.embedded.block.php",
				"meta.interpolation",
				"text.html.php",
				"variable.other.php"
			],
			"settings": {
				"foreground": "#EEEEEE"
			}
		},
		{
			"name": "Very Valuey (Undecorated)",
			"scope": [
				"storage",
				"keyword.other.namespace",
				"keyword.operator.assignment",
				"keyword.operator.increment",
				"keyword.operator.decrement",
				"punctuation.definition.bold",
				"punctuation.definition.italic",
				"punctuation.definition.underline",
				"punctuation.definition.strikethrough",
				"meta.brace.square",
				"punctuation.definition.begin.bracket.square",
				"punctuation.definition.end.bracket.square"
			],
			"settings": {
				"foreground": "#DD6677"
			}
		},
		{
			"name": "Moderately Valuey",
			"scope": [
				"entity.name.type",
				"entity.other.inherited-class",
				"support.type",
				"support.class",
				"keyword.type"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#EE9977"
			}
		},
		{
			"name": "Moderately Valuey (Undecorated)",
			"scope": [
				"keyword.operator.type",
				"keyword.operator.optional",
				"string.regexp constant.other.character-class",
				"string.regexp punctuation.definition.character-class",
				"string.regexp punctuation.character.set",
				"string.regexp support.other.escape.special",
				"string.regexp support.other.match.any"
			],
			"settings": {
				"foreground": "#EE9977"
			}
		},
		{
			"name": "Slightly Valuey (Undecorated)",
			"scope": [
				"constant.numeric",
				"keyword.other.unit",
				"variable.other.property",
				"variable.other.constant.property",
				"support.variable.property",
				"meta.object-literal.key",
				"entity.other.attribute-name",
				"string.regexp constant.other.character-class.set",
				"string.regexp constant.other.character-class.range",
				"string.regexp constant.character.set",
				"markup.bold",
				"markup.italic",
				"markup.underline",
				"markup.strikethrough"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#EECCAA"
			}
		},
		{
			"name": "Markup: italic",
			"scope": [
				"markup.italic"
			],
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"name": "Markup: bold",
			"scope": [
				"markup.bold"
			],
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markup: underline",
			"scope": [
				"markup.underline"
			],
			"settings": {
				"fontStyle": "underline"
			}
		},
		{
			"name": "Markup: strikethrough",
			"scope": [
				"markup.strikethrough"
			],
			"settings": {
				"fontStyle": "strikethrough"
			}
		},
		{
			"name": "Markup: heading",
			"scope": [
				"markup.heading"
			],
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"scope": "token.info-token",
			"settings": {
				"foreground": "#6796e6"
			}
		},
		{
			"scope": "token.warn-token",
			"settings": {
				"foreground": "#cd9731"
			}
		},
		{
			"scope": "token.error-token",
			"settings": {
				"foreground": "#f44747"
			}
		},
		{
			"scope": "token.debug-token",
			"settings": {
				"foreground": "#b267e6"
			}
		}
	],
	"semanticHighlighting": true,
	"semanticTokenColors": {
		"parameter": {
			"italic": true
		},
		"macro": {
			"foreground": "#EECCAA",
			"italic": true
		},
		"enumMember": {
			"foreground": "#AADDFF",
			"italic": true
		},
		"variable.global": {
			"foreground": "#EECCAA"
		}
	}
}