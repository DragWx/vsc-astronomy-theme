{
	"name": "Astronomy Light",
	"type": "light",
	"colors": {
		"editorPane.background": "#BBBBBB",
		"editor.background": "#eeeeee",
		"editor.foreground": "#222222",
		"editor.lineHighlightBackground": "#e9e9e9",
		"editor.selectionBackground": "#BBBBBB",
		"editorRuler.foreground": "#BBBBBB",
		"editorLineNumber.foreground": "#BBBBBB",
		"editorLineNumber.activeForeground": "#919191",
		"editorBracketMatch.background": "#919191",
		"editorBracketMatch.border": "#00000000",
		"editorIndentGuide.background": "#dddddd",
		"editorIndentGuide.activeBackground": "#BBBBBB",
		
		"statusBar.noFolderBackground": "#BBBBBB",
		"statusBar.noFolderForeground": "#919191",
		"statusBar.background": "#dddddd",
		"statusBar.foreground": "#919191",
		"statusBar.debuggingBackground": "#eed5c4",
		"statusBar.debuggingForeground": "#cc804d",
		
		"activityBar.background": "#BBBBBB",
		"activityBar.foreground": "#6b6b6b",
		"activityBarBadge.background": "#e69966",
		"activityBarBadge.foreground": "#222222",
		"sideBar.background": "#e9e9e9",
		"sideBar.foreground": "#6b6b6b",
		"sideBar.border": "#BBBBBB",
		"sideBarTitle.foreground": "#4499DD",
		"sideBarSectionHeader.foreground": "#5a73c0",
		"sideBarSectionHeader.background": "#dddddd",
		"list.focusBackground": "#eebb99",
		"list.focusForeground": "#222222",
		"list.activeSelectionBackground": "#eebb99",
		"list.activeSelectionForeground": "#222222",
		"list.inactiveSelectionBackground": "#d2d2d2",
		"list.highlightForeground": "#DD7733",

		"dropdown.background": "#c2def4",
		"dropdown.border": "#a2ccee",
		"dropdown.listBackground": "#dddddd",

		"input.background": "#f4d3bc",
		"input.border": "#eebb99",
		"input.placeholderForeground": "#e9a477",
		"focusBorder": "#b7b7b7",

		"breadcrumb.background": "#dddddd",
		"breadcrumb.foreground": "#6b6b6b",
		"breadcrumb.activeSelectionForeground": "#222222",
		"breadcrumb.focusForeground": "#222222",

		"titleBar.activeBackground": "#dddddd",
		"titleBar.activeForeground": "#6b6b6b",
		"menu.background": "#dddddd",
		"menu.foreground": "#5a5a5a",
		"menu.separatorBackground": "#919191",
		"menu.selectionBackground": "#e69966",
		"menu.selectionForeground": "#222222",
		"menubar.selectionBackground": "#eebb99",
		"menubar.selectionForeground": "#222222",

		"panel.background": "#e9e9e9",
		"panel.border": "#BBBBBB",
		"panelTitle.activeForeground": "#6b6b6b",
		"panelTitle.activeBorder": "#4499DD",

		"editorGroupHeader.tabsBackground": "#e9e9e9",
		"tab.activeBackground": "#eeeeee",
		"tab.activeForeground": "#6b6b6b",
		"tab.inactiveBackground": "#e9e9e9",
		"tab.border": "#00000000",

		"badge.background": "#eebb99",
		"badge.foreground": "#222222",
		"button.background": "#73b3e6",
		"button.foreground": "#222222"
	},
	"tokenColors": [
		{
			"name": "Comments\n",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#919191"
			}
		},
		{
			"name": "Strings",
			"scope": [
				"string",
				"punctuation.definition.string.begin",
				"punctuation.definition.string.end"
			],
			"settings": {
				"foreground": "#DD7733"
			}
		},
		{
			"name": "Strings: Escape Sequences",
			"scope": [
				"constant.character.escape"
			],
			"settings": {
				"foreground": "#DDAA88"
			}
		},
		{
			"name": "Very Logical",
			"scope": [
				"keyword.control",
				"keyword.operator.new",
				"keyword.other"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#4499DD"
			}
		},
		{
			"name": "Very Logical (Undecorated)",
			"scope": [
				"punctuation",
				"keyword.operator.comparison",
				"keyword.operator.relational",
				"keyword.operator.logical",
				"meta.brace"
			],
			"settings": {
				"foreground": "#4499DD"
			}
		},
		{
			"name": "Slightly Logical",
			"scope": [
				"constant.language",
				"source.css support.constant"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#2244AA"
			}
		},
		{
			"name": "Slightly Logical (Undecorated)",
			"scope": [
				"entity.name.function",
				"support.function",
				"entity.name.tag",
				"markup.heading"
			],
			"settings": {
				"foreground": "#2244AA"
			}
		},
		{
			"name": "Neutral",
			"scope": [
				"meta.template.expression",
				"string.unquoted.heredoc.php",
				"punctuation.definition.variable.php",
				"variable.other.php",
				"meta.embedded.block.php",
				"meta.interpolation",
				"text.html.php",
				"variable.other.php"
			],
			"settings": {
				"foreground": "#222222"
			}
		},
		{
			"name": "Very Valuey (Undecorated)",
			"scope": [
				"storage",
				"keyword.other.namespace",
				"keyword.operator.assignment",
				"keyword.operator.increment",
				"keyword.operator.decrement",
				"punctuation.definition.bold",
				"punctuation.definition.italic",
				"punctuation.definition.underline",
				"meta.brace.square",
				"punctuation.definition.begin.bracket.square",
				"punctuation.definition.end.bracket.square"
			],
			"settings": {
				"foreground": "#DD7733"
			}
		},
		{
			"name": "Moderately Valuey",
			"scope": [
				"entity.name.type",
				"entity.other.inherited-class",
				"support.type",
				"support.class",
				"keyword.type"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#BB5511"
			}
		},
		{
			"name": "Moderately Valuey (Undecorated)",
			"scope": [
				"keyword.operator.type",
				"keyword.operator.optional"
			],
			"settings": {
				"foreground": "#BB5511"
			}
		},
		{
			"name": "Slightly Valuey (Undecorated)",
			"scope": [
				"constant.numeric",
				"keyword.other.unit",
				"variable.other.property",
				"variable.other.constant.property",
				"support.variable.property",
				"meta.object-literal.key",
				"entity.other.attribute-name",
				"markup.bold",
				"markup.italic",
				"markup.underline"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#992200"
			}
		},
		{
			"name": "Markup: italic",
			"scope": [
				"markup.italic"
			],
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"name": "Markup: bold",
			"scope": [
				"markup.bold"
			],
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markup: underline",
			"scope": [
				"markup.underline"
			],
			"settings": {
				"fontStyle": "underline"
			}
		},
		{
			"name": "Markup: heading",
			"scope": [
				"markup.heading"
			],
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"scope": "token.info-token",
			"settings": {
				"foreground": "#6796e6"
			}
		},
		{
			"scope": "token.warn-token",
			"settings": {
				"foreground": "#cd9731"
			}
		},
		{
			"scope": "token.error-token",
			"settings": {
				"foreground": "#f44747"
			}
		},
		{
			"scope": "token.debug-token",
			"settings": {
				"foreground": "#b267e6"
			}
		}
	]
}