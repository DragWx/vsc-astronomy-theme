{
    "name": "astronomy-theme",
    "displayName": "Astronomy",
    "description": "A dichromatic warm/neutral/cool theme for Visual Studio Code",
    "publisher": "DragWx",
    "icon": "astronomy-icon.png",
    "version": "0.2.0",
    "engines": {
        "vscode": "^1.33.0"
    },
    "categories": [
        "Themes"
    ],
    "contributes": {
        "themes": [
            {
                "label": "Astronomy",
                "uiTheme": "vs-dark",
                "path": "./themes/astronomy-standard-color-theme.json"
            },
            {
                "label": "Astronomy Colorblind",
                "uiTheme": "vs-dark",
                "path": "./themes/astronomy-colorblind-color-theme.json"
            }
        ]
    },
    "private": true,
    "repository": {
        "type": "git",
        "url": "https://github.com/DragWx/vsc-astronomy-theme.git"
    },
    "dependencies": {
        "gulp": "3.9.1",
        "mustache": "^3.0.1"
    }
}
